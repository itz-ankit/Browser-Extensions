{
    "manifest_version": 3,
    "name": "Game Viewer",
    "version": "1.0",
    "description": "View all free games available.",
    "permissions": [
        "activeTab"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "images/controller.png"
        }
    },
    "icons": {
        "128": "images/controller.png"
    },
    "host_permissions": [
        "https://www.freetogame.com/*"
    ],
    "background": {
        "service_worker": "background.js"
    }
}